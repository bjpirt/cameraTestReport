{
  "name": "shutter-speed-report",
  "version": "1.0.0",
  "description": "Client-side web application for generating camera shutter speed service reports",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --outfile=public/dist/bundle.js",
    "dev": "esbuild src/index.tsx --bundle --outdir=public/dist --servedir=public --serve=8000",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "camera",
    "shutter-speed",
    "report",
    "photography"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "cypress": "^15.7.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  }
}
